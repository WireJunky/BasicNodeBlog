{
    "swagger": {
        "outputDirectory": "./Src/Application/Server/Middleware/SwaggerUI",
        "entryFile": "./Src/Server.ts",
        "host": "localhost:8080",
        "basePath": "/api/v1",
        "yaml": false            
    },
    "routes": {
