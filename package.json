{
  "name": "basicnodeblog",
  "version": "0.0.1",
  "description": "Basic blog engine serving static content",
  "main": "index.js",
  "scripts": {
    "client-prod": "./node_modules/.bin/webpack --config ./webpack.client.config.prod.js --progress --mode production",
    "client-dev": "./node_modules/.bin/webpack --config ./webpack.client.config.dev.js --progress --profile --display-error-details --mode development",
    "server-build": "node ./Build/prebuild.js && tsc",
    "server-rebuild": "node ./Build/prebuild.js && node ./Build/updateApi.js && tsc",
    "debug": "./node_modules/.bin/concurrently --kill-others \"tsc -w\" \"node ./node_modules/nodemon/bin/nodemon.js\"",
    "start-dev": "node --inspect-brk=5858 ./Dist/App/Server.js",
    "start-windows": "npm run server-build && cross-env NODE_PATH=./Dist/App DEBUG=express:* node ./Dist/App/Server.js",
    "pretest": "npm run build-server",
    "test": "jest",
    "preinstall": "node ./Build/preinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WireJunky/BasicNodeBlog.git"
  },
  "keywords": [
    "node",
    "blog",
    "static"
  ],
  "author": "wirejunky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WireJunky/BasicNodeBlog/issues"
  },
  "homepage": "https://github.com/WireJunky/BasicNodeBlog#readme",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@types/core-js": "^0.9.43",
    "@types/express-serve-static-core": "^4.11.1",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/loglevel": "^1.4.29",
    "@types/react-transition-group": "^2.0.6",
    "@types/winston": "^2.3.7",
    "autoprefixer": "^7.1.2",
    "body-parser": "^1.17.0",
    "browser-sync": "^2.21.0",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.1",
    "emitter": "0.0.5",
    "inversify": "^4.13.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "loglevel": "^1.4.1",
    "markdown-to-jsx": "^6.4.1",
    "mkdirp": "^0.5.1",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.4",
    "mobx-react-router": "^4.0.1",
    "morgan": "^1.8.1",
    "react": "^16.4.0",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
